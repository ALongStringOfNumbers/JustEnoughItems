pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                checkout scm
                sh "rm -rf build/libs"
            }
        }

        stage('Build') {
            steps {
                sh "JAVA_HOME=${env.JDK_16} ./gradlew test reobf -PBUILD_NUMBER=${env.BUILD_NUMBER} --no-daemon"
            }
        }

    }
}
